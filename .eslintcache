.state.hide = false;\n  }\n\n  openSignUpTab(event) {\n    event.preventDefault();\n    this.setState({\n      key: \"register\",\n    });\n  }\n\n  isValid(field, type) {\n    var login = this.state.login;\n\n    if (type === \"email\") {\n      login.userName = field.target.value;\n      }\n    if (type === \"password\") {\n      login.password = field.target.value;\n    }\n    this.setState({login: login});\n  }\n\n  submit(event){\n    event.preventDefault();\n    console.log(this.state.login);\n    localStorage.setItem('isLoggedIn', true);\n    localStorage.setItem('username', this.state.login.userName);\n\n    this.setState({ hide: true, show: false, key: \"login\" })\n    window.location.reload();\n\n  }\n\n  render() {\n    if (this.props.show) {\n      this.state.show = this.props.show;\n    }\n\n    return (\n      <div>\n        <div>\n          <Modal\n            show={this.state.show && !this.state.hide}\n            onHide={() => this.setState({ hide: true, show: false })}\n          >\n            {this.modalHeader()}\n            <Tabs\n              id=\"login-signup-tab\"\n              activeKey={this.state.key}\n              onSelect={(k) => this.setState({ key: k })}\n            >\n              <Tab eventKey=\"login\" title=\"Login\">\n                <Modal.Body>{this.showLogin()}</Modal.Body>\n                <Modal.Footer>\n                  <div>\n                    Don't have an account:\n                    <a\n                      href=\"\"\n                      onClick={this.openSignUpTab}\n                      className=\"signup-text\"\n                    >\n                      {\" \"}\n                      Sign Up\n                    </a>\n                  </div>\n                </Modal.Footer>\n              </Tab>\n              <Tab eventKey=\"register\" title=\"Register\">\n                <Modal.Body>{this.showSignUp()}</Modal.Body>\n              </Tab>\n            </Tabs>\n          </Modal>\n        </div>\n      </div>\n    );\n  }\n\n  modalHeader() {\n    return (\n      <Modal.Header>\n        <Modal.Title>Welcome to RentAway</Modal.Title>\n        <Button\n          variant=\"\"\n          onClick={() =>\n            this.setState({ hide: true, show: false, key: \"login\" })\n          }\n        >\n          <b>X</b>\n        </Button>\n      </Modal.Header>\n    );\n  }\n\n  showLogin() {\n    return (\n      <Form onSubmit={this.submit} className=\"login-modal\">\n        <FacebookLoginButton className=\"social-login\" align=\"center\" />\n        <GoogleLoginButton className=\"social-login\" align=\"center\" />\n\n        <div className=\"text-center pt3\">--- Or ---</div>\n        <Form.Group controlId=\"login-form\">\n          <div className=\"formgrp\">\n            <Form.Label>Email</Form.Label>\n            <Form.Control\n              type=\"email\"\n              placeholder=\"Enter Email\"\n              onChange={(item) => this.isValid(item, \"email\")}\n            />\n          </div>\n          <div className=\"formgrp\">\n            <Form.Label>Password</Form.Label>\n            <Form.Control\n              type=\"password\"\n              placeholder=\"Enter your password\"\n              onChange={(item) => this.isValid(item, \"password\")}\n            />\n          </div>\n        </Form.Group>\n        <Button type=\"submit\" className=\"btn-lg btn-blue btn-block\">\n          Login\n        </Button>\n      </Form>\n    );\n  }\n\n  showSignUp() {\n    return (\n      <div>\n        <Form className=\"login-modal\">\n          <Form.Group controlId=\"login-form\">\n            <div className=\"formgrp\">\n              <Form.Label>Name</Form.Label>\n              <Form.Control type=\"text\" placeholder=\"Enter your Name\" />\n            </div>\n            <div className=\"formgrp\">\n              <Form.Label>Email</Form.Label>\n              <Form.Control type=\"email\" placeholder=\"Enter Email\" />\n            </div>\n            <div className=\"formgrp\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control type=\"password\" placeholder=\"Enter your password\" />\n            </div>\n            <div className=\"formgrp\">\n              <Form.Label>Mobile Number</Form.Label>\n              <Form.Control\n                type=\"number\"\n                placeholder=\"Enter your mobile number\"\n              />\n            </div>\n          </Form.Group>\n          <Button\n            onClick={() => this.setState({ hide: true, show: false })}\n            className=\"btn-lg btn-blue btn-block\"\n          >\n            Register\n          </Button>\n        </Form>\n      </div>\n    );\n  }\n}\n\nexport default Login;\n","/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/App.js",[],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/index.js",[],["51","52"],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/home.js",[],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/item/addItem.js",["53"],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/item/ListItemDetails.js",[],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/search/Search.js",["54"],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/navbar/Navbar.js",[],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/footer.js",["55","56"],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/category/categories.js",[],"/Users/dibyaranjan/Documents/qafficient/Halloween/rentaway-app/src/component/item/ListItems.js",["57"],{"ruleId":"58","severity":1,"message":"59","line":29,"column":5,"nodeType":"60","endLine":29,"endColumn":15},{"ruleId":"58","severity":1,"message":"59","line":64,"column":7,"nodeType":"60","endLine":64,"endColumn":17},{"ruleId":"61","severity":1,"message":"62","line":85,"column":21,"nodeTy